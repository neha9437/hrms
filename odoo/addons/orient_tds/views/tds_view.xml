    	<record id="view_employee_provisional_form" model="ir.ui.view">
            <field name="name">employee.provisional.form</field>
            <field name="model">employee.provisional</field>
            <field name="arch" type="xml">

            	<form string="Add/Modify Employees Provisional Investments">
            		<sheet>
                        <!-- <div class="oe_button_box" name="button_box">
                            <button name="toggle_active" type="object" groups="hr.group_hr_user"
                                    class="oe_stat_button" icon="fa-archive">
                                <field name="active" widget="boolean_button"
                                    options='{"terminology": "active"}'/>
                            </button>
                        </div> -->
            			<group>
		    				<group>
                                <field name="name" invisible="1"/>
                                <field name="employee" 
                                groups="orient_hr_resignation.group_finance_employee"/>
                                <field name="year" options="{'no_open': True}" required="1" widget="selection"/>
                                <field name="particulars_name" required="0" options="{'no_open': True,'nocreate':True}" widget="selection"/>
                                <field name="section" required="0" options="{'no_open': True,'nocreate':True}" widget="selection"/>
                                <!-- <field name="active" /> -->
                                <field name="particulars" domain="[('section','=',section)]" required="0" options="{'no_open': True,'nocreate':True}" widget="selection"/>
                                <field name="description" class="oe_inline" force_save="1" readonly="1"/>
                                <field name="date_of_submission" readonly="1"/>
                                <field name="amount" required="1"/>
                                <field name="check_exists" invisible="1"/>
                                <field name="period_exists" invisible="1"/>
                                <field name="from_date" attrs="{'invisible':[('period_exists','=',False)],'required':[('period_exists','=',True)]}"/>
                                <field name="to_date" attrs="{'invisible':[('period_exists','=',False)],'required':[('period_exists','=',True)]}"/>
                            </group>
                            <group>
                                <label string="" colspan="2"/>
                                <label string="" colspan="2"/>
                                <button string="Search" type="object" name="search_recs" colspan="2" class="btn-primary"/>
                            </group>
        				</group>                       
                            <button string="Add" type="object" name="add_recs" class="btn-primary"/>
			    <label string="" colspan="2"/>
                            <button string="View" type="object" name="view_recs" class="btn-primary"/>
                         <!-- <group>
                            
                        </group> -->
                        <newline/>
                        <!-- <tree string="Annual Details" editable="bottom" open="false" mode="tree"> -->
                            <field name="provisional_lines" editable="top" attrs="{'invisible':[('check_exists','=',False)]}" context="{'provisional_id': active_id}" create="false">
                                <tree string="Provisional Lines" editable="bottom" mode="tree" create="false" >
                                    <field name="provisional_id" invisible="1"/>
                                    <field name="particulars_name" readonly="1"/>
                                    <field name="employee" invisible="1"/>
                                    <field name="amount" readonly="1"/>
                                    <field name="status" readonly="1"/>
                                    <button string="Edit" type="object" name="select_rec" class="btn-primary"/>
                                    <!-- <button string="Delete" type="object" name="add_recs"/> -->
                                    <button string="Attach File" type="object" name="attach_file" class="btn-primary"/>
                                    <!-- <field name="doc_attachment" widget="binary"/> -->
                                    <!-- <field name="attachments" />  -->
                                    <field name="section" invisible="1"/>
                                    <field name="particulars" invisible="1"/>
                                    <field name="description" invisible="1"/>
                                    <field name="date_of_submission" invisible="1"/>
                                    <field name="period_exists" invisible="1"/>
                                </tree>
                            </field>
        			</sheet>
        		</form>
    		</field>
		</record>

        <record id="ir_attachment_view_search_inherit_hr_recruitment" model="ir.ui.view">
            <field name="name">ir.attachment.search.inherit.recruitment</field>
            <field name="model">ir.attachment</field>
            <field name="mode">primary</field>
            <field name="inherit_id" ref="base.view_attachment_search"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='create_date']" position="after">
                    <field name="index_content" string="Content"/>
                </xpath>
                <xpath expr="//filter[@name='my_documents_filter']" position="attributes">
                    <attribute name='invisible'>1</attribute>
                </xpath>
                <xpath expr="//filter[@name='url_filter']" position="attributes">
                    <attribute name='invisible'>1</attribute>
                </xpath>
                <xpath expr="//filter[@name='binary_filter']" position="attributes">
                    <attribute name='invisible'>1</attribute>
                </xpath>
            </field>
        </record>

